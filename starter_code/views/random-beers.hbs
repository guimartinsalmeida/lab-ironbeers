<h1>Randonbeers</h1>
<div id="random-beer" class="card" style="width: 33.333%;">
  {{#each beers}}

  <img class="card-img-top" src="{{this.image_url}}" alt="">
  <div id="random-beer-text">
    <div class="card-body">
      <h5 class="card-title">{{this.name}}</h5>
      <p class="card-text">{{this.description}}</p>
      <p class="card-text">{{this.tagline}}</p>
      <ul>
        {{#each this.food_pairing}}
        <li>{{this}}</li>
        {{/each}}
      </ul>
      <p>{{this.brewers_tips}}</p>

    </div>
    {{/each}}
  </div>
</div>